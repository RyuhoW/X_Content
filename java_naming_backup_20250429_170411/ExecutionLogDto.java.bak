package com.xcontent.model.dto;

import com.xcontent.model.CacheScheduleExecutionLog;
import lombok.Data;
import java.time.LocalDateTime;
import java.util.List;

@Data
public class ExecutionLogSummary {
    private Long scheduleId;
    private int totalExecutions;
    private int successfulExecutions;
    private int failedExecutions;
    private Double averageExecutionTime;
    private List<CacheScheduleExecutionLog> recentErrors;
}

@Data
public class ExecutionMetrics {
    private String cacheName;
    private LocalDateTime periodStart;
    private LocalDateTime periodEnd;
    private double averageExecutionTime;
    private double maxExecutionTime;
    private double minExecutionTime;
    private double successRate;
    private int totalProcessedKeys;
    private int totalFailedKeys;
}
